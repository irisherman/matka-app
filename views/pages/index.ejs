                <img class="card-img-top" src="<%= matkad[i].pildiUrl %>">
                <div class="card-body">
                    <h4 class="card-title"><%= matkad[i].nimetus %></h4>
                    <p>
                        <%= matkad[i].kirjeldus %>                        
                    </p>
                    <p>
                        Osalejaid: <%= matkad[i].osalejad.length %>
                    </p>
                    <a href="/registreerumine/<%= matkad[i].id %>" class="btn btn-primary">Registreeru</a>
                </div>
            </div>
            <% } %>
        </div>
    </div>
</body>

</html>
