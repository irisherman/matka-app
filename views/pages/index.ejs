    <div class="paisSektsioon">
        <div class="container">
            <div class="row pt-2 ">
                <div class="col-12 col-md-5 d-flex">
                    <img class="paisLogo" src="/assets/klubilogo.png" alt="">
                    <div class="paisNimi">
                        MATKA KLUBI
                    </div>
                
    
        <div class="col-md-12 col-md-7 pais-menyy">
           
            <%- include("../partials/menyy.ejs") %>
        </div>
            </div>
            <div class="row">
                <div class="col-12 col-lg-6 paisSektsioon-pealkirjad">
                    <div class="rida">
                        <h2>Matkaklubi</h2>
                        <img class="liblikas1" src="./assets/Butterfly2.png" alt="">
                    </div>
                    <div class="rida">
